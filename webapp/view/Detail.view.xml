<mvc:View
	xmlns="sap.m"
	xmlns:mvc="sap.ui.core.mvc"
	controllerName="UI_FIVE.controller.Detail"
	xmlns:custom="UI_FIVE.control">
	<Page
		title="{i18n>detailPageTitle}" titleAlignment="Center"
		showNavButton="true" navButtonPress="onPressNavBack" class="sapUiResponsiveMargin">
		<ObjectHeader title="{invoice>ProductName}" intro="{invoice>ShipperName}"
			responsive="true"
			fullScreenOptimized="true"
			number="{
			parts:['invoive>ExtendedPrice', 'view>/currency'],
			type: 'sap.ui.model.type.Currency',
			formatOptions: {showMeasure: false}
			}"
			numberUnit="{view>/currency}">
			<attributes>
				<ObjectAttribute title="{i18n>quantityTitle}" text="{invoice>Quantity}" />
				<ObjectAttribute
					title="{i18n>dateTitle}"
					text="{
                        path: 'invoice>ShippedDate',
                        type: 'sap.ui.model.type.Date',
                        formatOptions: {
                            style: 'long',
                            source: { pattern: 'yyyy-MM-ddTHH:mm:ss' }
                        }
                    }" />
			</attributes>
		</ObjectHeader>
		<custom:ProductRating
			id="rating"
			class="sapUiSmallMarginBeginEnd"
			change="onRatingChange" />
	</Page>
</mvc:View>

<!-- xmlns:custom = "UI_FIVE.control"-->